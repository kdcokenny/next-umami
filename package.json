{
  "name": "next-umami",
  "author": "Gianluca De Marchi - hi@gixnluca.dev",
  "version": "0.0.5", 
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs", 
      "require": "./dist/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:types": "tsup --dts",
    "typecheck": "tsc --noEmit",
    "lint": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "tsup && tsup --dts",
    "prepublishOnly": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gixnluca/next-umami.git"
  },
  "homepage": "https://github.com/gixnluca/next-umami#readme",
  "bugs": {
    "url": "https://github.com/gixnluca/next-umami/issues"
  },
  "keywords": [
    "umami",
    "nextjs",
    "analytics"
  ],
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.14",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "next": "^14.2.5",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.2.4",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
